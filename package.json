{
  "name": "jsf-resit",
  "private": true,
  "version": "0.1.7",
  "type": "module",
  "scripts": {
    "test": "node test.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier -w src/App.jsx && prettier -w src/**/*.jsx",
    "lint": "eslint src/App.jsx",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.9.0",
    "react-router-dom": "^6.12.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.1.4",
    "eslint": "^8.38.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "postcss": "^8.4.24",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.2",
    "vite": "^4.2.0"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.jsx": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "description": "- Prettier\r - ESLint\r - Lint-staged",
  "main": "index.html",
  "keywords": [
    "resit",
